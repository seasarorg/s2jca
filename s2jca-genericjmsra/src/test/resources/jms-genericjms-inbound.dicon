<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.3//EN"
	"http://www.seasar.org/dtd/components23.dtd">
<components namespace="jms-genericjms-cf">
	<include path="j2ee.dicon"/>
	<include path="jca.dicon"/>
	<include path="jms-genericjms-ra.dicon"/>

	<component
		class="org.seasar.jca.deploy.impl.MessageEndpointDeployer"
	>
		<property name="activationSpecClassName">"com.sun.genericra.inbound.ActivationSpec"</property>
		<initMethod name="setProperty">
			<arg>"connectionFactoryProperties"</arg>
			<arg>"BrokerURL=tcp://localhost:61616"</arg>
		</initMethod>
		<initMethod name="setProperty">
			<arg>"destinationType"</arg>
			<arg>"javax.jms.Queue"</arg>
		</initMethod>
		<initMethod name="setProperty">
			<arg>"destinationProperties"</arg>
			<arg>"PhysicalName=foo"</arg>
		</initMethod>
	</component>

	<component
		class="org.seasar.jca.inbound.MessageEndpointFactoryImpl"
	>
		<property name="deliveryTransacted">true</property>
	</component>

	<component instance="prototype"
		class="org.seasar.jca.inbound.GenericJMSTest$MessageListenerImpl"
	/>
</components>
